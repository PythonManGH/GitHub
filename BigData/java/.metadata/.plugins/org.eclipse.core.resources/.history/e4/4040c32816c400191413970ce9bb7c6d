package liu.hadoop.java;

public class ArrayTest {
	public static void main(String[] args) {
		
		//静态初始化
		int[] arr1; // 声明一个数组
		arr1 = new int[]{1,2,3,4,5,6}; //初始化一个数组
		int[] arr2 = new int[]{1,2,3,4,5,6}; //同时声明和初始化
		
		//动态初始化
		String[] arr3 = new String[3];
		arr3[0] = "liujun";
		arr3[1] = "mingming";
		arr3[2] = "tingting";
	}
}
